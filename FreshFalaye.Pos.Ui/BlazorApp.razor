
@using FreshFalaye.Pos.Ui.Components.Layout
@using FreshFalaye.Pos.Ui.Components.Pages
@using Microsoft.AspNetCore.Components.Routing
<Router AppAssembly="@typeof(BlazorApp).Assembly"
                AdditionalAssemblies="new[] { typeof(ReceiptHtml).Assembly }">
